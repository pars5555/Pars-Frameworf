{
    "VERSION": "1.1.28",
    "ENVIRONMENT": "dev",
    "smarty": {
        "caching": false,
        "cache_lifetime": 3600,
        "compile_check": true
    },
    "data_dir_name": "sys_data",
    "display_errors": 1,
    "error_reporting": "E_ALL",
    "log_level": "DEBUG",
    "log": {
        "dump_request": true,
        "dump_input": true
    },
    "mysql": {
        "host": "127.0.0.1",
        "user": "sys",
        "pass": "sys123",
        "name": "sys"
    },     
    "login": {
        "type": "cookie",  
        "domain": "subdomain",
        "expire_minutes": 1440,
        "salt": "%fd!fg@#gfj$gh!@gfh#hj$j%#",
        "params": {
            "user_id": "sys_uid",
            "user_type": "sys_utype",
            "user_hash": "sys_uhash"
        }
    },    
    "languages": {
        "url_first_part": {
            "enable": true,
            "available_lang_codes": ["hy", "en", "ru"]
        },
        "expire_seconds": 31104000,
        "default_language_code": "hy"
    }
}